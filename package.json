{
  "name": "jarvis",
  "version": "1.0.0",
  "description": "JARVIS - Advanced Audio Visualizer and AI Assistant",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "node node_modules/electron/cli.js .",
    "electron:dev": "set NODE_ENV=development&& node node_modules/electron/cli.js .",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [],
  "author": "Ashutosh Kumar Singh",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "axios": "^1.6.0",
    "dat.gui": "^0.7.9",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.4.0",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "vite": "^7.2.4"
  },
  "build": {
    "appId": "com.jarvis.ai",
    "productName": "Jarvis",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron.js",
      "preload.js"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}